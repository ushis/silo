/* Documents */

body.documents div.attachments table td a.icon-attachment,
body.documents div.cvs table td a.icon-file {
  position: relative;
  padding-left: 20px;
  display: block;
}

body.documents div.attachments table td a.icon-attachment:before,
body.documents div.attachments table td a.icon-attachment:after,
body.documents div.cvs table td a.icon-file:before,
body.documents div.cvs table td a.icon-file:after {
  position: absolute;
  left: 0;
  font-size: 1.45em;
  line-height: 1em;
}

body.documents div.attachments table td a.icon-attachment:before,
body.documents div.cvs table td a.icon-file:before {
  top: 0;
  color: #fff;
  text-shadow: 0 1px 1px rgba(0, 0, 0, .4);
}

body.documents div.attachments table td a.icon-attachment:after,
body.documents div.cvs table td a.icon-file:after {
  top: 1px;
  color: #5c5c5c;
}

body.documents div.cvs table td a.icon-file:after {
  top: -1px;
}

