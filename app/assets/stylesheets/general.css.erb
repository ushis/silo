/**
 * general.css.erb
 *
 * General style definitions
 */
/* Elements */
body {
  background-color: #323232;
  min-width: 1130px;
}

form.button_to input[type=password] {
  display: none;
}

a, form.button_to input[type=submit] {
  margin: 0;
  padding: 0;
  font-size: 1em;
  color: #d0d0d0;
  background-color: transparent;
  border: 0 none transparent;
  cursor: pointer;
}

a.disabled {
  cursor: default;
}

h1 {
  text-transform: uppercase;
}

h2 {
  padding: 8px 0;
  font-size: 1.2em;
  color: #444;
}

em {
  font-style: italic;
}

strong {
  font-weight: bold;
}

div.clear {
  height: 0;
  width: 0;
  visibility: hidden;
  clear: both;
}

/* Everybody needs one. */
#wrapper {
  float: left;
  width: 100%;
  padding: 0 10px 10px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

/* Header */
#header {
  width: 100%;
  float: left;
  padding: 0 20px;
  line-height: 60px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#header h1 {
  float: left;
  margin: 18px 0 0 -9px;
}

#header h1 a {
  display: block;
  width: 81px;
  height: 30px;
  overflow: hidden;
  overflow: hidden;
  margin-bottom: 20px;
  text-indent: -99999px;
  background: transparent url(<%= asset_data_uri 'logo.svg' %>) 0 0 no-repeat;
  background-size: 81px 30px;
}

#header h1 a:before {
  font-size: 2em;
  color: #f5f5f5;
  text-shadow: 0 1px #000;
}

#user-tools {
  float: right;
  margin: 16px -20px 0 0;
  padding: 4px;
  position: relative;
  border-radius: 3px;
}

#user-tools li {
  float: right;
  font-size: 12px;
}

#user-tools li a {
  display: block;
  line-height: 24px;
  padding: 0 10px;
  color: #7a7a7a;
  text-shadow: 0 1px 0 rgba(0,0,0,.5);
}

#user-tools li a:hover {
  color: #999;
}

#user-tools li a:after {
  margin-left: 7px;
}

/* Navigation */
#nav {
  float: left;
  width: 100%;
  height: 35px;
  overflow: hidden;
  padding-left: 10px;
  font-size: .8em;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#nav ul {
  float: left;
  width: 100%;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#nav > ul > li {
  float: left;
  width: 17.5%;
  max-width: 250px;
  padding: 0 3px 0 2px;
  line-height: 18px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#nav ul li:first-child {
  padding-left: 0;
}

#nav ul li a,
#nav ul #current-list {
  font-size: .9em;
  display: block;
  padding: 8px 20px 9px 20px;
  text-align: center;
  text-transform: uppercase;
  background: -moz-linear-gradient(top, #515151 0%, #464646 100%);
  background: -webkit-linear-gradient(top, #515151 0%, #464646 100%);
  background: -o-linear-gradient(top, #515151 0%, #464646 100%);
  background: -ms-linear-gradient(top, #515151 0%, #464646 100%);
  background: linear-gradient(to bottom, #515151 0%, #464646 100%);
  border-radius: 6px 6px 0 0;
  text-shadow: 0 1px 0px rgba(0, 0, 0, .5);
}

#nav ul li.lists,
#nav ul #current-list {
  float: right;
  font-size: 1em;
  width: auto;
  padding-right: 0;
}

#nav ul li.lists a {
  border-top-right-radius: 0;
}

#nav ul li.lists a:before {
  margin-right: 7px;
  line-height: 1.75em;
}

#nav ul #current-list {
  max-width: 400px;
  padding: 8px 10px 9px 15px;
  line-height: 18px;
  border-radius: 0 6px 0 0;
}

#nav ul #current-list div {
  float: left;
  color: #d0d0d0;
  line-height: 1.5em;
}

#nav ul #current-list div.title {
  display: none;
  margin-right: 12px;
  text-transform: none;
  color: #888;
}

#nav ul #current-list.active div.title {
  display: block;
}

#nav ul #current-list div a {
  padding: 0;
  overflow: hidden;
  white-space: nowrap;
}

#nav ul #current-list div.title a {
  display: inline;
  max-width: 200px;
  margin-left: 3px;
  text-transform: none;
  font-size: 1em;
  background: none;
}

body.lists #nav ul #current-list div.title a {
  color: #444;
  text-shadow: 0 1px 0 rgba(255,255,255,.5);
}

#nav ul #current-list div.open a {
  border-radius: 3px;
  height: 20px;
  width: 20px;
  background: #676767;
  box-shadow: inset 0 -1px 0 rgba(0,0,0,.4), inset 0 1px 0 rgba(255,255,255,.1);
}

#nav ul #current-list div.open a:hover {
  background: #606060;
}

#nav ul #current-list div.open a:before {
  margin: 0 99999px 0 5px;
}

body.lists #nav #current-list div.open a {
  background: #cdcdcd;
  color: #666;
  text-shadow: 0 1px 0 rgba(255,255,255,.5);
  box-shadow: inset 0 -1px 0 rgba(0,0,0,.2), inset 0 1px 0 rgba(255,255,255,.1);
}

body.lists #nav #current-list div.open a:hover {
  background: #cdcdcd;
  color: #444;
}

body.experts #nav li.experts a,
body.partners #nav li.partners a,
body.employees #nav li.partners a,
body.references #nav li.references a,
body.lists #nav li.lists a,
body.lists #nav #current-list {
  color: #444;
  background: #dbdbdb;
  text-shadow: 0 1px 0px #efefef;
}

/* Content */
#content-wrapper {
  clear: both;
  padding: 0 10px 10px 10px;
  border-radius: 4px 0 4px 4px;
  background-color: #464646;
}

#content {
  clear: both;
  padding: 10px 0;
  background-color: #dbdbdb;
  border-radius: 4px;
}

/* Sidebar */
#sidebar {
  position: absolute;
  width: 80px;
  padding: 4px 10px 0 10px;
  font-size: .7em;
}

#sidebar ul li {
  padding: 4px 0 10px;
  border-bottom: 1px solid #c9c9c9;
  box-shadow: 0 1px 0 #e9e9e9;
}

#sidebar ul li a {
  display: block;
  padding: 0;
  color: #555;
  text-shadow: 0 1px 0 rgba(255,255,255,.66);
  text-align: center;
}

#sidebar ul li a:hover {
  display: block;
  padding: 0;
  color: #333;
  text-shadow: 0 1px 0 rgba(255,255,255,.33);
  text-align: center;
}

body.index #sidebar ul li.index,
body.new #sidebar li.new {
  margin: -4px -10px 0 0;
  padding-top: 9px;
  background-color: #f9f9f9;
  border: 0 none transparent;
  border-radius: 4px 0 0 4px;
  box-shadow: -1px 1px 1px rgba(0, 0, 0, .2);
}

body.index #sidebar ul li.index a,
body.new #sidebar li.new a{
  color: #444;
}

#sidebar ul li [class^="icon-"]:before,
#sidebar ul li [class*=" icon-"]:before {
  display: block;
  font-size: 1.9em;
}

/* Main */
#main {
  margin: 0 10px 0 100px;
  padding: 18px 20px;
  background-color: #f9f9f9;
  border-radius: 4px;
  box-shadow: 0 1px 1px rgba(0, 0, 0, .2);
}

body.index #main {
  border-top-left-radius: 0;
}

/* Main Header & Form Wrapper */
#main div.header,
div.form-wrapper {
  float: left;
  position: relative;
  width: 100%;
  margin: 0 0 10px 0;
  padding: 10px;
  background: #dbdbdb;
  border-radius: 4px;
  box-shadow: inset 0 1px 2px rgba(0, 0, 0, .2);
}

#main div.header,
#main div.form-wrapper {
  margin: -8px 0 -8px -10px;
}

#main div.header {
  overflow: hidden;
  margin-bottom: 30px;
}

#main div.header h2 {
  padding-left: 24px;
  background-position: left 1px;
}

/* Filter */
#filter div.row {
  clear: left;
  float: left;
  width: 600px;
  padding: 10px 0;
}

#filter div.row.name {
  padding-top: 0;
  border-bottom: 1px solid #bbb;
  box-shadow: 0 1px 0 #efefef;
}

#filter div.row.q {
  padding-bottom: 0;
}

#filter input[type=text] {
  width: 193px;
}

#filter div.citizenship input[type=text] {
  margin-right: 0;
}

#filter #q {
  width: 396px;
}

#filter input[type=submit] {
  float: right;
}

#filter div.need-help {
  float: left;
  position: relative;
  cursor: pointer;
  padding: 2px 7px 1px 7px;
  margin: 3px 0 0 7px;
  font-size: .7em;
  font-weight: bold;
  color: #777;
  border: none;
  background: #dbdbdb;
  text-shadow: 0 1px 0px rgba(255, 255, 255, .4);
  border-radius: 3px;
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, .3), 0 1px 1px rgba(0, 0, 0, .4);
}

#filter div.need-help:hover {
  color: #444;
}

#filter div.need-help:before {
  content: ' ';
  position: absolute;
  top: 48%;
  right: 100%;
  border: solid transparent;
  width: 0;
  height: 0;
  margin-top: -3px;
  border-width: 4px;
  border-right-color: rgba(0, 0, 0, .5);
}

#filter div.need-help:after {
  content: ' ';
  position: absolute;
  top: 48%;
  right: 100%;
  border: solid transparent;
  width: 0;
  height: 0;
  margin-top: -4px;
  border-width: 4px;
  border-right-color: #dbdbdb;
}

/* Tabs */
#main div.header ul.tabs {
  position: absolute;
  bottom: 0;
  font-size: .8em;
}

#main div.header ul.tabs li {
  cursor: pointer;
  text-align: center;
  float: left;
  line-height: 2.2em;
  padding: 0;
  margin-right: 3px;
  background: -moz-linear-gradient(top, #515151 0%, #464646 100%);
  background: -webkit-linear-gradient(top, #515151 0%, #464646 100%);
  background: -o-linear-gradient(top, #515151 0%, #464646 100%);
  background: -ms-linear-gradient(top, #515151 0%, #464646 100%);
  background: linear-gradient(to bottom, #515151 0%, #464646 100%);
  color: #dbdbdb;
  background: #555;
  border-radius: 4px 4px 0 0;
  text-shadow: 0 1px 0px rgba(0,0,0,.5);
}

#main div.header ul.tabs li a {
  display: block;
  padding: 0px 15px;
  text-shadow: none;
}

body.show #main div.header ul.tabs li.show,
body.documents #main div.header ul.tabs li.documents,
body.employees #main div.header ul.tabs li.employees,
body.experts-list #main div.header ul.tabs li.experts-list,
body.partners-list #main div.header ul.tabs li.partners-list {
  background: #f9f9f9;
}

body.show #main div.header ul.tabs li.show a,
body.documents #main div.header ul.tabs li.documents a,
body.employees #main div.header ul.tabs li.employees a,
body.experts-list #main div.header ul.tabs li.experts-list a,
body.partners-list #main div.header ul.tabs li.partners-list a {
  color: #3b3b3b;
}

#main div.header.details {
  min-height: 66px;
}

#main div.header h2,
#main div.form-wrapper h2 {
  padding: 0;
  line-height: 1em;
  text-shadow: 0 1px 0px #efefef;
  margin-bottom: 6px;
}

#main div.form-wrapper h2 {
  padding: 10px 0 0 10px;
}

body.new #main h2 {
  display: none;
}

body.edit #main h2 {
  margin: 0 0 20px -5px;
  padding: 0;
}

body.edit #main h2[class^="icon-"]:before,
body.edit #main h2[class*=" icon-"]:before{
  margin-right: 5px;
  font-size: 1.25em;
  line-height: 1em;
}

#main div.header h3 {
  padding: 0;
  font-size: .75em;
  color: #888;
  line-height: 1em;
  text-shadow: 0 1px 0px #efefef;
  margin-bottom: 10px;
}

#main div.header ul.tools {
  position: absolute;
  top: 5px;
  right: 10px;
  font-size: .8em;
}

#main div.header ul.tools li {
  float: left;
  margin-left: 16px;
}

#main div.header ul.tools li a {
  display: block;
  height: 24px;
  margin: 0;
  padding: 0;
  line-height: 24px;
  color: #666;
  text-shadow: 0 1px 0 #efefef;
  font-size: .9em;
  font-weight: normal;
  background-color: transparent;
  background-position: left center;
  background-repeat: no-repeat;
  box-shadow: 0 0 0 transparent;
}

#main div.header ul.tools li a.listable {
  opacity: 0;
  -webkit-transition: opacity .5s;
  -moz-transition: opacity .5s;
}

#main div.header ul.tools li a.listable.ready {
  opacity: 1;
}

#main div.header ul.tools li a:hover {
  color: #111;
}

#main div.header ul.tools li [class^="icon-"]:before,
#main div.header ul.tools li [class*=" icon-"]:before,
#main div.header ul.tools li a.listable:before {
  padding-right: 7px;
  font-size: 1.25em;
  vertical-align: bottom;
}

#main div.header ul.tools li a.listable span.active,
#main div.header ul.tools li a.listable.active span.inactive {
  display: none;
}

#main div.header ul.tools li a.listable.active span.active {
  display: inline;
}

#main div.header ul.tools li a.listable:before {
  font-family: FontAwesome;
  content: '\f212';
}

#main div.header ul.tools li a.listable.active:before {
  content: '\f213';
}

/* Tables */
table {
  width: 100%;
  color: #5b5b5b;
  clear: left;
  font-size: .75em;
  border-collapse: separate;
  border-spacing: 0 8px;
  position: relative;
}

table td {
  padding: 7px 10px 5px 10px;
  background-color: #efefef;
  box-shadow: 0 1px 1px rgba(0, 0, 0, .2);
}

table td:first-child {
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
  background-color: #f7f7f7;
  border-left: none;
}

table td:last-child {
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}

table td a {
  color: #555;
}

table td.icons {
  padding: 7px;
}

table td.icons a {
  display: block;
  width: 18px;
  height: 18px;
  overflow: hidden;
  background-position: left center;
  background-repeat: no-repeat;
}

table td.icons input[type=submit].delete {
  padding: 0;
  margin: 0;
  text-shadow: none;
  box-shadow: none;
  border: none;
  background: none;
}

table td.icons [class^="icon-"]:before,
table td.icons [class*=" icon-"]:before {
  font-size: 1.25em;
}

table td.del {
  width: 20px;
  padding: 0 5px 0 4px;
  vertical-align: middle;
  text-align: center;
}

table td.del a {
  display: block;
  width: 20px;
  height: 20px;
  overflow: hidden;
  color: #666;
}

table td.del a:hover {
  color: #444;
}

table td.del a:before {
  font-size: 1.2em;
}

table thead tr {
  line-height: 0;
}

table thead th {
  text-align: left;
  border-spacing: 0;
  background: none;
  box-shadow: none;
  font-size: .8em;
  padding: 0 5px 0 0;
  color: #bbb;
  text-shadow: 0 1px 0px rgba(255,255,255,.2);
  padding: 0 0 0 5px;
}

table thead th span {
  float: right;
}

body.index #main td a.listable {
  position: relative;
  float: right;
  height: 19px;
  width: 28px;
  margin: -2px 10px -1px 0;
  padding: 2px 0 1px 0;
}

body.index #main td a.listable + a {
  margin-right: 40px;
}

body.index #main td a.listable:hover {
  cursor: pointer;
  color: #222;
  background-color: #eee;
  border-radius: 3px;
  box-shadow: inset 0 1px 0 rgba(255,255,255,.5),
              inset 0 -1px 0 rgba(0,0,0,.2),
              0 1px 1px rgba(0,0,0,.075);
}

body.index #main td a.listable:hover:before {
  font-family: FontAwesome;
  font-weight: normal;
  font-style: normal;
  display: inline-block;
  text-decoration: inherit;
  content: "\f212";
  font-size: 12px;
  margin: 0px 0 0 5px;
}

body.index #main td a.listable.active:hover:before {
  content: "\f213";
}

body.index #main td a.listable:hover:after {
  content: ' ';
  position: absolute;
  top: 50%;
  right: -10px;
  border: solid transparent;
  width: 0;
  height: 0;
  margin-right: 1px;
  margin-top: -5px;
  border-width: 5px;
  border-left-color: #ececec;
}

body.index #main td a.listable div.marker {
  position: absolute;
  top: 50%;
  right: -10px;
  margin-top: -3px;
  height: 6px;
  width: 6px;
  border-radius: 3px;
  background-color: #f7f7f7;
  box-shadow: inset 0 1px 1px rgba(0,0,0,.3);
}

body.index #main td a.listable.active div.marker:after {
  content: ' ';
  position: absolute;
  top: 1px;
  left: 1px;
  height: 4px;
  width: 4px;
  border-radius: 2px;
  background-color: #666;
}

#main table td a {
  display: block;
}

#main table td.attachments a {
  height: 18px;
  width: 0;
  overflow: hidden;
  position: relative;
  padding-left: 12px;
}

#main table td.attachments a:before,
#main table td.attachments a:after {
  position: absolute;
  left: 0;
  font-size: 1.45em;
  line-height: 1em;
}

#main table td.attachments a:before {
  top: 0;
  color: #fff;
  text-shadow: 0 1px 1px rgba(0, 0, 0, .4);
}

#main table td.attachments a:after {
  top: 1px;
  color: #5c5c5c;
}

#main table td.cvs a {
  border-radius: 2px;
  padding: 0;
  margin-right: 8px;
  display: inline;
  text-transform: uppercase;
  color: #444;
  font-size: .9em;
}

#main table td.cvs a {
  display: inline;
  position: relative;
  padding-left: 15px;
}

#main table td.cvs a[class^="icon-"]:before, 
#main table td.cvs a[class*=" icon-"]:before,
#main table td.cvs a[class^="icon-"]:after, 
#main table td.cvs a[class*=" icon-"]:after {
  position: absolute;
  left: 0;
  top: -2px;
  font-size: 1.45em;
  line-height: 1em;
}

#main table td.cvs a[class^="icon-"]:before, 
#main table td.cvs a[class*=" icon-"]:before {
  color: #fff;
  text-shadow: 0 1px 1px rgba(0, 0, 0, .4);
}

#main table td.cvs a[class^="icon-"]:after, 
#main table td.cvs a[class*=" icon-"]:after {
  color: #777;
  top: -3px;
}

#main table th.attachments {
  width: 14px;
}

#main table td.male {
  border-left: 2px solid #7c99b6;
}

#main table td.female {
  border-left: 2px solid #b67c84;
}

/* Show Tables */
body.show #main div.left {
  float: left;
  width: 60%
}

body.show #main div.right {
  float: right;
  width: 37%
}

body.show #main div.info {
  float: left;
  position: relative;
  width: 100%;
  padding: 0;
  margin-bottom: 10px;
  border-radius: 4px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

body.show #main div.info.contacts,
body.show #main div.info.addresses {
  background: none;
  box-shadow: none;
  padding: 0;
}

body.show #main div.info div.row {
  float: left;
  width: 100%;
  padding: 10px 0;
  border-bottom: 1px solid #e5e5e5;
  box-shadow: 0 1px 1px rgba(255, 255, 255, 1);
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

body.show #main div.info div.row:first-child {
  padding-top: 0;
}

body.show #main div.info div.row.last {
  border-bottom: none;
  box-shadow: none;
  padding-bottom: 0;
}

body.show #main div.info div.row div {
  float: left;
  width: 100%;
}

body.show #main div.info div.row h4 {
  color: #7a7a7a;
  font-size: .7em;
  text-shadow: 0 1px 0 rgba(255, 255, 255, .9);
}

body.show #main div.info p {
  color: #444;
  font-size: .75em;
}

body.show #main div.info table p {
  color: #444;
  font-size: 1em;
}

body.show #main div.info div.row div.comments p {
  font-size: .9em;
}

body.show #main div.info div.row p:empty:after,
body.show #main div.info div.row div.markdown:empty:after {
  content: '-';
}

body.show #main div.info div.row div.tools {
  float: right;
  width: auto;
  text-align: right;
  margin: -4px 3px -7px 0;
}

body.show #main div.info div.row div.tools a.tool {
  font-size: .75em;
  color: #777;
  text-shadow: 0 1px 0 #EFEFEF;
  font-weight: normal;
  background-color: transparent;
  background-position: left center;
  background-repeat: no-repeat;
  box-shadow: 0 0 0 transparent;
}

body.show #main div.info div.row div.tools a.tool:before {
  margin: 0 6px 0 15px;
}

body.show #main div.right div.tab div.form-wrapper {
  float: left;
  position: relative;
  width: 100%;
  margin: 0;
  padding: 0;
  background: #DBDBDB;
  border-radius: 4px;
  box-shadow: inset 0 1px 2px rgba(0, 0, 0, .2);
}

body.show #main div.right div.tab div.form-wrapper div textarea,
body.show #main div.right div.tab div.form-wrapper div input[type=text] {
  margin-bottom: 10px;
}

body.show #main div.right div.tab div.form-wrapper div select {
  margin: 0;
  width: 150px;
}

body.show #main div.right div.forms {
  margin-top: 35px;
  position: relative;
}

body.show #main div.right div.forms div.tab {
  float: left;
  width: 100%;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

body.show #main div.right div.forms ul.tabs {
  width: 100%;
  position: relative;
  top: 0;
  left: 0;
  display: block;
  margin-top: -29px;
  z-index: 99;
}

body.show #main div.right div.forms ul.tabs li {
  float: left;
  cursor: pointer;
  background: transparent;
  margin-right: 3px;
  border-radius: 4px 4px 0 0;
  padding: 2px;
}

body.show #main div.right div.forms ul.tabs li a {
  font-size: .75em;
  color: #777777;
  text-shadow: 0 1px 0 rgba(255,255,255,.5);
  padding: 10px;
}

body.show #main div.right div.forms ul.tabs li.ui-state-active {
  background: #DBDBDB;
  box-shadow: inset 0 1px 2px rgba(0, 0, 0, .2);
  color: #525252;
  border-bottom: 5px solid #DBDBDB;
  margin-bottom: -10px;
  z-index: 99;
}

body.show #main div.info table.within {
  margin-bottom: -5px;
}

body.show #main div.info table.within td {
  background: none;
  border-spacing: 0;
  box-shadow: none;
  padding: 0 0 4px 0;
  color: #444;
  border-radius: 0;
  border-bottom: 1px solid #E5E5E5;
  box-shadow: 0 1px 1px rgba(255, 255, 255, 1);
}

body.show #main div.info table.within td a {
