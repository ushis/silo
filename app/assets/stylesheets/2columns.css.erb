body.experts.show #main,
body.experts.documents #main,
body.partners.show #main,
body.partners.documents #main,
body.list_items #main,
body.employees #main {
  padding-right: 10px;
  background: rgb(255,255,255);
  background: -moz-linear-gradient(left,  rgba(255,255,255,1) 12%, rgba(238,238,238,1) 60%);
  background: -webkit-gradient(linear, left top, right top, color-stop(12%,rgba(255,255,255,1)), color-stop(60%,rgba(238,238,238,1)));
  background: -webkit-linear-gradient(left,  rgba(255,255,255,1) 12%,rgba(238,238,238,1) 60%);
  background: -o-linear-gradient(left,  rgba(255,255,255,1) 12%,rgba(238,238,238,1) 60%);
  background: -ms-linear-gradient(left,  rgba(255,255,255,1) 12%,rgba(238,238,238,1) 60%);
  background: linear-gradient(to right,  rgba(255,255,255,1) 12%,rgba(238,238,238,1) 60%);
  box-shadow: 0 1px 2px rgba(0,0,0,.33);
}

/* Left */
#main div.sidebar.details {
  position: relative;
  float: left;
  z-index: 2;
  width: 280px;
  padding: 0;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.sidebar.details h2 {
  position: relative;
  float: left;
  width: 95%;
  padding: 0 0 0 22px;
  margin-bottom: 20px;
  font-size: 15px;
  line-height: 18px;
}

#main div.sidebar.details h3 {
  float: left;
  color: #aaa;
  text-shadow: 0 1px 0 white;
  line-height: 15px;
  font-size: 11px;
}

#main div.sidebar.details h3.updated_at {
  margin-top: -15px;
  margin-bottom: 20px;
}

#main div.sidebar.details h2:before {
  position: absolute;
  top: 0;
  left: 0;
  font-size: 15px;
}

#main div.sidebar.details h4 {
  float: left;
  display: block;
  width: 100%;
  color: #999;
  font-size: .7em;
  padding: 0 2px 4px 0;
  font-weight: bold;
  text-shadow: 0 1px 0 #FFF;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.sidebar.details p,
#main div.sidebar.details a {
  position: relative;
  float: left;
  width: 100%;
  color: #777;
  text-shadow: 0 1px 0 white;
  line-height: 15px;
  font-size: 12px;
  padding-right: 15px;
  margin-bottom: 10px;
}

#main div.sidebar.details div.markdown {
  position: relative;
  float: left;
  width: 100%;
}

#main div.sidebar.details div.markdown p,
#main div.sidebar.details div.markdown ul {
  position: relative;
  float: left;
  color: #777;
  text-shadow: 0 1px 0 white;
  line-height: 15px;
  font-size: 12px;
  padding-right: 15px;
}

#main div.sidebar.details div.markdown ul li {
  color: #777;
  text-shadow: 0 1px 0 white;
  line-height: 15px;
  font-size: 12px;
  padding-right: 15px;
}

#main div.sidebar.details div.markdown p:before {
  margin-right: 5px;
}

#main div.sidebar.details ul.tools {
  position: relative;
  float: left;
  width: 100%;
  margin-bottom: 30px
}

#main div.sidebar.details ul.tools li {
  position: relative;
  float: left;
  width: 50%;
  padding: 0 0 0 5px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.sidebar.details ul.tools li:nth-child(2n+1) {
  padding: 0 5px 0 0;
}

#main div.sidebar.details ul.tools li a {
  display: block;
  float: left;
  height: 22px;
  width: 100%;
  margin: 0 5px 5px 0;
  padding: 0 5px;
  line-height: 22px;
  color: #6b6b6b;
  text-shadow: 0 1px 0 #FFF;
  font-size: 11px;
  font-weight: normal;
  text-align: center;
  background-color: transparent;
  background-position: left center;
  background-repeat: no-repeat;
  box-shadow: 0 0 0 transparent;
  background: #EFEFEF;
  border-radius: 3px;
  border: 1px solid #d2d2d2;
  box-shadow: 0 1px 0 #fbfbfb, inset 0 1px 0 #FFF;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.sidebar.details ul.tools li a span:before {
  margin-right: 5px;
}

#main div.sidebar.details ul.tools li a:hover {
  color: #5b5b5b;
  background: #f5f5f5;
}

#main div.sidebar.details ul.tools li a:before {
  margin-right: 7px;
}

#main div.sidebar.details ul.tools li a.disabled,
#main div.sidebar.details ul.tools li.open a.active {
  opacity: .66;
  cursor: default;
}

#main div.sidebar.details ul.tools li a span.inactive,
#main div.sidebar.details ul.tools li a span.active {
  display: block;
}

#main div.sidebar.details ul.tools li a.active span.inactive {
  display: none;
}

#main div.sidebar.details ul.tools li a.disabled:hover {
  color: #6b6b6b;
  background: #EFEFEF;
}

/* Right */
#main div.tabbed-content {
  position: relative;
  display: block;
  padding-left: 300px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.tabbed-content div.content {
  position: relative;
  float: left;
  width: 100%;
  padding: 10px;
  background: #fff;
  box-shadow: 0 1px 2px rgba(0, 0, 0, .33);
  border-radius: 0 4px 4px 4px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  z-index: 2;
}

#main div.tabbed-content div.content a {
  color: #555;
}

#main div.tabbed-content .tools {
  position: absolute;
  top: -30px;
  right: 7px;
}

#main div.tabbed-content ul.tools li,
#main div.tabbed-content div.tools a {
  float: left;
}

#main div.tabbed-content .tools a {
  display: block;
  height: 22px;
  margin-left: 5px;
  padding: 0 10px;
  line-height: 22px;
  color: #6B6B6B;
  text-shadow: 0 1px 0 white;
  font-size: 11px;
  font-weight: normal;
  text-align: center;
  background: #EFEFEF;
  border-radius: 3px;
  border: 1px solid #D2D2D2;
  box-shadow: 0 1px 0 #FBFBFB, inset 0 1px 0 white;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.tabbed-content .tools a:hover {
  color: #5B5B5B;
  background: whiteSmoke;
}

#main div.tabbed-content .tools a:nth-last-child(n+2) {
  margin-right: 5px;
}

#main div.tabbed-content .tools a:before {
  margin-right: 7px;
  font-size: 1.25em;
  vertical-align: bottom;
}

#main div.tabbed-content .tools a.icon-plus:before {
  font-size: 1em;
  margin-right: 5px;
}

#main div.sidebar.details .lists {
  margin-bottom: 20px;
}

#main div.sidebar.details ul.lists,
#main div.sidebar.details ul.lists li {
  float: left;
  width: 100%;
  text-align: left;
}

#main div.sidebar.details ul.lists {
  margin-top: 5px;
}

#main div.sidebar.details ul.lists li a {
  width: auto;
  text-align: left;
}

#main div.sidebar.details ul.lists li a:before {
  margin: 0 7px 0 0;
  padding: 0;
  font-size: 14px;
}

#main div.tabbed-content ul.tabs {
  position: relative;
  float: left;
  z-index: 1;
  height: 30px;
}

#main div.tabbed-content ul.tabs li {
  position: relative;
  z-index: 1;
  padding: 0;
  cursor: pointer;
  text-align: center;
  float: left;
  line-height: 1em;
  font-size: 12px;
  margin-right: 5px;
  background: #ECECEC;
  box-shadow: 0 1px 2px rgba(0, 0, 0, .33), inset 0 -2px 0 rgba(0, 0, 0, .025), inset 0 1px 0 rgba(0, 0, 0, .03);
  border-radius: 4px 4px 0 0;
}

#main div.tabbed-content ul.tabs li a {
  display: block;
  padding: 9px 15px;
  color: #666;
  text-shadow: 0 1px 0px rgba(255, 255, 255, .8);
}

#main div.tabbed-content div.info.empty p {
  padding-top: 10px;
  color: #555;
}

#main div.tabbed-content div.info h4 {
  color: #AFAFAF;
  font-size: 11px;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 1);
  margin: -5px 0 -10px 5px;
}

#main div.tabbed-content div.info.addresses {
  margin-bottom: 30px;
}

#main div.tabbed-content div.info table td {
  background: transparent;
  box-shadow: 0 0 0 0 transparent;
  border-bottom: 1px solid #eaeaea;
  padding: 0 5px 5px 5px;
  border-radius: 0;
}

#main div.tabbed-content div.info table td p {
  color: #555;
  font-size: 12px;
}

#main div.tabbed-content div.info.docs table {
  padding-left: 5px;
  padding-right: 5px;
}

#main div.tabbed-content div.info.docs table td {
  padding: 7px 10px 5px 10px;
  background: #f0f0f0;
  box-shadow: 0 1px 1px rgba(0, 0, 0, .2);
  border: 0 none transparent;
}

#main div.tabbed-content div.info.docs table td:first-child {
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
  background-color: #F9F9F9;
}

#main div.tabbed-content div.info.docs table td:last-child {
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}

#main div.tabbed-content div.info.docs table td a.icon-file,
#main div.tabbed-content div.info.docs table td a.icon-attachment {
  position: relative;
  padding-left: 20px;
  display: block;
}

#main div.tabbed-content div.info.docs table td a.icon-file:before,
#main div.tabbed-content div.info.docs table td a.icon-attachment:before,
#main div.tabbed-content div.info.docs table td a.icon-file:after,
#main div.tabbed-content div.info.docs table td a.icon-attachment:after {
  position: absolute;
  left: 0;
  font-size: 1.45em;
  line-height: 1em;
}

#main div.tabbed-content div.info.docs table td a.icon-file:after {
  top: -1px;
}

#main div.tabbed-content div.info.docs table td a.icon-file:before,
#main div.tabbed-content div.info.docs table td a.icon-attachment:before {
  top: 0;
  color: white;
  text-shadow: 0 1px 1px rgba(0, 0, 0, .4);
}

#main div.tabbed-content div.info.docs table tr.headline:nth-child(n+2) td {
  padding-top: 20px;
}

#main div.tabbed-content div.info.docs table tr.headline h4 {
  font-size: 11px;
}

#main div.tabbed-content div.content div.left,
#main div.tabbed-content div.content div.right {
  width: 50%;
  padding: 15px 10px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  background: #FFF;
}

#main div.tabbed-content div.content div.left {
  float: left;
  padding-right: 20px;
  border-right: 1px solid #EEE;
}

#main div.tabbed-content div.content div.right {
  float: left;
  margin-left: -1px;
  padding-left: 20px;
  border-left: 1px solid #EEE;
}

#main div.tabbed-content div.content ul.info {
  position: relative;
  float: left;
  width: 100%;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
}

#main div.tabbed-content div.content ul.info li {
  position: relative;
  float: left;
  width: 50%;
  padding: 10px 5px;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  border-bottom: 1px solid #eaeaea;
}

#main div.tabbed-content div.content ul.info li:last-child {
  border-bottom: 0 none transparent;
  padding-bottom: 0;
}

#main div.tabbed-content div.content ul.info li h4 {
  color: #AFAFAF;
  font-size: .7em;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 1);
  padding: 0;
}

#main div.tabbed-content div.content ul.info li p {
  color: #555;
  font-size: 12px;
  padding: 5px;
  min-height: 19px;
  padding: 0;
}

#main div.tabbed-content table {
  padding-top: 5px;
}

#main div.tabbed-content table td {
  background-color: #f1f1f1;
}

#main div.tabbed-content table td:first-child {
  background-color: #f9f9f9;
}
